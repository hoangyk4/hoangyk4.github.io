@model TodoListApp.WebApp.DBModel.Task

<div class="modal fade" id="editModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Detail Tasks</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label asp-for="ID" cla6ss="control-label">@Model.ID</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="Name" cla6ss="control-label">@Model.Name</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="Status" class="control-label">@Model.Status</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="Owner" class="control-label">@Model.Owner</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="CategoryID" class="control-label">@Model.CategoryID</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="IsCompleted" class="control-label">@Model.IsCompleted</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="DueDate" class="control-label">@Model.DueDate</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="OverDue" class="control-label">@Model.OverDue</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="CreatedDate" class="control-label">@Model.CreatedDate</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="CreatedBy" class="control-label">@Model.CreatedBy</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="UpdatedDate" class="control-label">@Model.UpdatedDate</label>
                    </div>
                    <div class="form-group">
                        <label asp-for="UpdatedBy" class="control-label">@Model.UpdatedBy</label>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>